public class UnitCircle 
{
    public static void main(String[] args)
    {
        System.out.println("Radians: (cos, sin)");
        
        System.out.println("0.0: 1.0, 0.0");
        
        System.out.println(Math.PI/2 + ": 0.0, 1.0");
        
        System.out.println(Math.PI + ": -1.0, 0.0");
    }
}
