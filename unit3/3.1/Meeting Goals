import java.util.Scanner;

public class Goals
{
    public static void main(String[] args)
    {
        
        // Ask for goal amount
        Scanner input = new Scanner(System.in);
        System.out.println("Enter your goal:");
        int goalAmnt = input.nextInt();
        // Ask for actual amount
        System.out.println("Enter your actual amount:");
        int goalAct = input.nextInt();
        // Compare numbers by creating three booleans
        boolean overGoal = goalAmnt < goalAct;
        boolean notMeet = goalAmnt > goalAct;
        boolean metGoal = goalAmnt == goalAct;
        // Display results as instructed
        System.out.println("Went over goal? " + overGoal);
        System.out.println("Did not meet goal? " + notMeet);
        System.out.println("Met goal exactly? " + metGoal);
    }
}
