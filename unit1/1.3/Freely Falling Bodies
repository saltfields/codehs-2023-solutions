public class FallingBodies
{
    public static void main(String[] args)
    {
        int t = 23; // time = 23 seconds
        double g = 9.8; // multiplier of gravity
        double h = 0.5 * g * (t*t); // height finder
        
        System.out.print("The height is ");
        System.out.print(h);
        System.out.println(" meters.");
        
        double v = g * t; // velocity finder
        
        System.out.print("The velocity is ");
        System.out.print(v);
        System.out.print(" meters per second.");
    }
}
